<div class="row">
  <div class="col-md-12">
    <h1>Events#index</h1>
  </div>
</div>
<div class="row">
  <div class="col-md-6">
    <h3>Upcoming Events</h3>
    <% @upcoming_events.each do |event| %>
      <%= render event %>
    <% end %>
  </div>
  <div class="col-md-6">
    <h3>Previous Events</h3>
    <% @previous_events.each do |event| %>
      <%= render event %>
    <% end %>
  </div>
</div>

